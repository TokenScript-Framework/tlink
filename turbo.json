{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"]
    },
    "lint": {},
    "type-check": {},
    "dev": {
      "cache": false,
      "persistent": true
    },
    "blink-twitter-inject#dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["@repo/blinks#build", "@repo/token-kit#build"]
    },
    "clean": {
      "cache": false
    }
  }
}
